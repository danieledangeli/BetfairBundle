{
    "name": "erlangb/betfair-bundle",
    "type": "bundle",
    "description": "Symfony bundle for using Betfair APIs",
    "keywords": ["betfair", "betting", "api", "betfair php", "sport api"],
    "homepage": "http://github.com/SimpleBus/AsynchronousBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele D'Angeli",
            "email": "dangeli88.daniele@gmail.com",
            "homepage": "http://www.danieledangeli.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.0",
        "erlangb/betfair": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "psr-4" : { "Erlangb\\BetfairBundle\\" : "src" }
    },
    "autoload-dev": {
        "psr-4" : { "Erlangb\\BetfairBundle\\Tests\\" : "tests" }
    }
}